#include<stdio.h>
int main(){
    int n=5;
    int i,j;
    int x=n;
    int y,t;
    for(i=n;i>=1;i--){
        t=x;
        y=i+1;
        for(j=n;j>=i;j--){
            printf("%2d",t);
            t=t-y;
            y++;
        }
        x=x+i-1;
        printf("\n");
    }
    return 0;
    
}

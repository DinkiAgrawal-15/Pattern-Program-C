#include<stdio.h>
int main(){

    
    int n=5;
    int i,j;
    int x=1;
    for(i=1;i<=n;i++){
        
        for(j=n;j>=1;j--){
            if(i>=j){
                printf("%2d",x++);
                
            }else{
                printf(" ");
            }
        
        }
        printf("\n");
    }
return 0;
}
